{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "https://api.themoviedb.org/3",
        "APP_ENV": "development",
        "ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "https://api.themoviedb.org/3",
        "APP_ENV": "preview",
        "ENV": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://api.themoviedb.org/3",
        "APP_ENV": "production",
        "ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
